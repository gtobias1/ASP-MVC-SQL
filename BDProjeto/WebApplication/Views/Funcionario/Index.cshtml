@model IEnumerable<BDProjeto.DTO.ExemploBD.Usuarios>

@{
    ViewBag.Title = "Funcionários";
}

<h2>Funcionários</h2>

<p>
    @Html.ActionLink("Cadastrar funcionário", "Cadastrar")
</p>
<table class="table">
    <tr>
        <!-- <th>
            @Html.DisplayNameFor(model => model.USUARIO_ID)
        </th> -->
        <th>
            @Html.DisplayNameFor(model => model.NOME)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CARGO)
        </th>
        <th>
            Data de admissão
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <!-- <td>
                        @Html.DisplayFor(modelItem => item.USUARIO_ID)
             </td> -->
            
            <td>
                @Html.DisplayFor(modelItem => item.NOME)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CARGO)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DATAINSERCAO)
            </td>
            <td>
                @Html.ActionLink("Editar", "Editar", new { id = item.USUARIO_ID }) |
                @Html.ActionLink("Detalhes", "Detalhes", new { id = item.USUARIO_ID }) |
                @Html.ActionLink("Excluir", "Excluir", new { id = item.USUARIO_ID })
            </td>
        </tr>
    }

</table>
